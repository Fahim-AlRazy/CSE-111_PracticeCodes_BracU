import java.util.Arrays;
import java.util.Scanner;
public class task3a{
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    System.out.print("Enter the length of an array : ");
    int n = sc.nextInt();
    int[] arr1= new int[n];
    for(int i=0;i<n;i++){
      System.out.print("Enter a number : ");
    arr1[i]= sc.nextInt();
    }
    
    
  }
}