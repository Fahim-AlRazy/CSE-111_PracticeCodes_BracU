//Task_3
import java.util.Scanner;
public class Task_3{
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    
    
    while(true)
    { int n = sc.nextInt();
      if(n>=0)
      { 
        int Sqr = n*n;
        System.out.println("Square of number = "+Sqr);
      }
      else
      {
        
        System.out.println("Negative number");
        break;
      }
    }
    
  }
}