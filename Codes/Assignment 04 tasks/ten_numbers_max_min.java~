import java.util.Scanner;
public class ten_numbers_max_min{
  public static coid main(String[] args){
    Scanner sc= new Scanner(System.in);
    System.out.print("Enter 10 numbers :");
    int max=0,min=0;
    int num;
    for(int i=1;i<=10;i++){
    num=scnextInt();
    if(num>max){
    max=num;
    }
    if(num<min){
    min=num;
    }
    }  
    System.out.println("Max is "+max+" Min is "+min);
  
  }
}